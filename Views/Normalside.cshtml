@using System.Globalization
@inherits umbraco.MacroEngines.DynamicNodeContext
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage

@{
    Layout = "Master.cshtml";
		
}


<div id="container" class="normal">
	
	

	@Umbraco.RenderMacro("TopImages")

    
    <article>
		<div id="content" class="main">
		
	        <div class="clearfix spacing"></div>
		        <div class="main gu6">
		        	<div class="row">
						<div class="col gu4">
							<h1 class="normal">@Model.Content.Name</h1>
							<div id="actions">
								<a href="#" class="shareholder yellowbutton"></a>
								@*Umbraco.RenderMacro("GetLocation")*@
								<a href="javascript:window.print();" class="print yellowbutton"></a>
									
								@{Html.RenderPartial("Sharer");}
							</div>
							<div class="clearfix"></div>
							<div class="description">							
								
								@Umbraco.Field("description", convertLineBreaks: true)
							</div>	
							<div class="bodytext">							
								@Model.Content.GetPropertyValue("bodyText")
							</div>
						</div>
						
						<div class="col gu2">
							
							
							<div class="clearfix spacing"></div>
							@Umbraco.RenderMacro("VidsteDu")
							
							<div class="clearfix spacing"></div>	
							<div class="event-cal">
		  
			   				 	<a href="/events"><h4>Eventkalender</h4></a>
		    					@Umbraco.RenderMacro("ListLatestEvents", new {nodeId="2277", limit="10", startIndex=0})	
		    
							</div>	
								
							
							
								
							
						</div>
		        	</div>
		        </div>
		</div>
    </article>
</div><!-- end #container -->